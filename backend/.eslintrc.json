{
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "rules": {
        "no-console": 1
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}